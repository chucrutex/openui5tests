{
   "_version": "1.0.0",
   "sap.app": {
      "id": "OpenUI5Tests",
      "type": "application"
   },
   "sap.ui5": {
      "dependencies": {
         "minUI5Version": "1.75.0",
         "libs": {
            "sap.ui.core": {},
            "sap.m": {},
            "sap.f": {},
            "sap.ui.unified": {}
         }
      },
      "rootView": {
         "viewName": "OpenUI5Tests.view.App",
         "type": "XML",
         "async": true,
         "id": "app"
      },
      "models": {
         "i18n": {
            "type": "sap.ui.model.resource.ResourceModel",
            "settings": {
               "bundleName": "OpenUI5Tests.i18n.i18n",
               "supportedLocales": ["pt-BR"],
               "fallbackLocale": "pt-BR"
            }
         }
      },
      "resources": {}
   }
}